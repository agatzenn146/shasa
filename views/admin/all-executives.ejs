                  <% executives.forEach(executive => { %>
  <tr>
    <td><%= executive.first_name %> <%= executive.last_name %></td>
    <td><%= executive.email %></td>
    <td><%= executive.position %></td>
    <td><img src="/uploads/<%= executive.photo %>" width="70"></td>
    <td>
      <a href="/admin/edit-executive/<%= executive.id %>">Edit</a>
      <form action="/admin/delete-executive/<%= executive.id %>" method="POST">
        <button type="submit">Delete</button>
      </form>
    </td>
  </tr>
<% }) %>

                
                    
